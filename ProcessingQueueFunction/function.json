{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "msg",
      "type": "queueTrigger",
      "direction": "in",
      "queueName": "itemsqueue",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "$return",
      "type": "cosmosDB",
      "databaseName": "processes",
      "collectionName": "processeditems",
      "createIfNotExists": true,
      "connectionStringSetting": "DbConnectionString",
      "direction": "out"
    }
  ]
}
